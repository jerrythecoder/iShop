<!-- Modal Dialog -->
<div class="modal fade" id="cancelCheckout" tabindex="-1" role="dialog" aria-labelledby="cancelCheckout">
	<div class="modal-dialog modal-sm" role="document">
		<div class="modal-content i-modal-content">
			<div class="modal-body i-modal-body">
				Checkout hasn't been completed yet. Sure to cancel?
			</div>
			<div class="modal-footer i-center-parent">
				<button type="button" class="btn btn-default i-btn-xsm" data-dismiss="modal">Continue</button>
				<a type="button" class="btn btn-primary i-btn-xsm" 
						href="${flowExecutionUrl}&_eventId=cancel">
					Yes
				</a>
			</div>
		</div>
	</div>
</div>